{
   "manifest_version": 3,
   "name": "FIG",
   "version": "1.0",
   "description": "Generates fake information based on inrelevant information to confuse any trackers on websites",
   "action": {
    "default_popup": "popup/popup.html"
   },
   "permissions": ["activeTab", "scripting"],
   "icons": {
    "48": "icons/icon48.png"
   },
     "contents-scripts":  [
    {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }
   ],
   "background":    {
    "service_worker": "background.js"
   }
}
 
